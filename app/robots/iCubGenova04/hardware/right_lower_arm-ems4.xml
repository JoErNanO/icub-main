<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

 <!-- Initialization file for EMS 4 - Right Lower Arm, 12 dof plus skin  -->

<devices robot="iCubGenova04" build="1">

  <!--  Motion Control -->
  <device name="right_lower_arm_mc" type="embObjMotionControl">
      <params file="general.xml" />
      <params file="hardware/electronics/right_lower_arm-ems4-eln.xml" />
      <params file="hardware/mechanicals/right_lower_arm-ems4-mec.xml" />
  </device>
 
        <!--  Skin -->
  <device name="right_lower_arm_skin" type="embObjSkin">
      <params file="general.xml" />
      <params file="hardware/electronics/right_lower_arm-ems4-eln.xml" />
      <!--<param name="skinCanIds_canPort1"> </param> --> <!--Attention: insert can addres in decreasing order -->
      <param name="SkinCanIds_canPort2"> 14 13 12 11 10 9 8 7 </param>  <!--Attention: insert can addres in decreasing order -->
  </device>  

<!--  mais --> 
  <device name="right_hand_mais" type="embObjAnalogSensor">
      <params file="general.xml" />
      <params file="hardware/electronics/right_lower_arm-ems4-eln.xml" />
      <params file="hardware/analog/right_lower_arm-ems4-mais.xml" />
  </device>
</devices>
